import java.util.*;
// Java program to check if
// linked list is circular
public class Main

{  
    
    /* Link list Node */
    static class Node{
    int data;
    Node next;
    }
    
    /*This function returns true if given linked
list is circular, else false. */
    static boolean isCircular(Node head){
        
        // An empty linked list is circular
        if(head == null)
            return true;
            
        // Next of head
        Node nextNode = head.next;
        
        // This loop would stop in both cases (1) If
    // Circular (2) Not circular
        while(nextNode != null && nextNode != head)
           nextNode = nextNode.next;
           
        // If loop stopped because of circular
    // condition
        return (nextNode == head);
    }
    
    // Utility function to create a new node.
    static Node makeNode(int data){
        Node temp = new Node();
        temp.data = data;
        temp.next = null;
        return temp;
    }
    
    /* Driver code*/
	public static void main(String[] args) {
	    
	     /* Start with the empty list */
		Node head =  makeNode(1);
		head.next = makeNode(2);
		head.next.next = makeNode(3);
		head.next.next.next = makeNode(4);
		System.out.println(isCircular(head)?"Circular":"Not Circular");
		
		// Making linked list circular
		head.next.next.next.next = head;
		System.out.println(isCircular(head)?"Circular":"Not Circular");
		
	}
}



class Main{

public static void main(String []args)
{
    int a[] = {5 , 4 ,3 ,2 ,6,8 ,10,1};
    int n=8,i,j,temp;
    for(i=0;i<n;i++){
        for( j=0;j<n-1;j++){
            if(a[j]>a[j+1]){
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
    for( i=0;i<n;i++){
        System.out.printf("%d ",a[i]);
    }

    
}
}
